@article{blank_how_2014,
	title = {How to protect eyewitness memory against the misinformation effect: A meta-analysis of post-warning studies},
	volume = {3},
	issn = {22113681},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S2211368114000230},
	doi = {10.1016/j.jarmac.2014.03.005},
	shorttitle = {How to protect eyewitness memory against the misinformation effect},
	abstract = {Four decades of research and hundreds of studies speak to the power of post-event misinformation to bias eyewitness accounts of events (see e.g. Loftus’ summary, 2005). A subset of this research has explored if the adverse influence of misinformation on remembering can be undone or at least reduced through a later warning about its presence. We metaanalyzed 25 such post-warning studies (including 155 effect sizes) to determine the effectiveness of different types of warnings and to explore moderator effects. Key findings were that (1) post-warnings are surprisingly effective, reducing the misinformation effect to less than half of its size on average. (2) Some types of post-warning (following a theoretical classification) seem to be more effective than others, particularly studies using an enlightenment procedure (Blank, 1998). (3) The post-warning reduction in the misinformation effect reflects a specific increase in misled performance (relative to no warning), at negligible cost for control performance. We conclude with a discussion of theoretical and practical implications.},
	pages = {77--88},
	number = {2},
	journaltitle = {Journal of Applied Research in Memory and Cognition},
	author = {Blank, Hartmut and Launay, Céline},
	urldate = {2019-07-19},
	date = {2014-06},
	langid = {english},
	file = {Blank und Launay - 2014 - How to protect eyewitness memory against the misin.pdf:/Users/carlottareinhardt/Zotero/storage/FC4JLKVM/Blank und Launay - 2014 - How to protect eyewitness memory against the misin.pdf:application/pdf},
}

