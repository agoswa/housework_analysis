	langid = {english},
	file = {Tse et al. - 2014 - A longitudinal study of stress-induced hippocampal.pdf:/Users/carlottareinhardt/Zotero/storage/BPWYPWKC/Tse et al. - 2014 - A longitudinal study of stress-induced hippocampal.pdf:application/pdf},
}

